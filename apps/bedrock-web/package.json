{
  "name": "apps-bedrock-web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "NODE_ENV=production bun run server.ts",
    "typecheck": "react-router typegen && tsc --noEmit",
    "lint": "bunx @biomejs/biome check app scripts server.ts package.json tsconfig.json react-router.config.ts vite.config.ts",
    "format": "bunx @biomejs/biome format --write app scripts server.ts package.json tsconfig.json react-router.config.ts vite.config.ts",
    "test": "bun test",
    "db:migrate": "bun run scripts/migrate.ts",
    "db:seed": "bun run scripts/seed.ts",
    "worker": "bun run scripts/worker.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.990.0",
    "@aws-sdk/s3-request-presigner": "^3.990.0",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-router/node": "7.12.0",
    "@react-router/serve": "7.12.0",
    "better-auth": "^1.4.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "isbot": "^5.1.31",
    "kysely": "^0.28.11",
    "kysely-postgres-js": "^3.0.0",
    "lucide-react": "^0.564.0",
    "pg-boss": "^12.11.2",
    "postgres": "^3.4.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router": "7.12.0",
    "tailwind-merge": "^3.4.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.1",
    "@react-router/dev": "7.12.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/bun": "^1.3.9",
    "@types/node": "^22",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
